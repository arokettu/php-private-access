{
    "name": "sandfoxme/private-access",
    "description": "Simple and fast methods to read private properties and call private methods",
    "keywords": ["debug"],
    "homepage": "https://sandfox.dev/php/private-access.html",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Smirnov",
            "role": "developer",
            "email": "sandfox@sandfox.me",
            "homepage": "https://sandfox.me/"
        }
    ],
    "support": {
        "source": "https://gitlab.com/sandfox/php-private-access",
        "issues": "https://gitlab.com/sandfox/php-private-access/-/issues",
        "docs": "https://php-private-access.readthedocs.io/"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sandfox\\Debug\\PrivateAccess\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">= 5.6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^4.8.36 | >= 6.0 < 10",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "*"
    }
}
